{
  "name": "ichhabrecht/composer-git-flow-plugin",
  "description": "This Composer plugin enables the possibility to switch between Git Flow branches `on the fly`",
  "type": "composer-plugin",
  "authors": [
    {
      "name": "Nicole Cordes",
      "email": "typo3@cordes.co"
    }
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "composer-plugin-api": "^1.0"
  },
  "require-dev": {
    "composer/composer": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "IchHabRecht\\GitFlow\\": "src/GitFlow/"
    }
  },
  "extra": {
    "class": "IchHabRecht\\GitFlow\\Plugin"
  }
}
